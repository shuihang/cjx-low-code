{
  "private": true,
  "packageManager": "pnpm@8.14.1",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "scripts": {
    "dev": "vite",
    "build": "pnpm run -C internal/build start",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "docs:preview": "pnpm run -C docs preview",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist"
  },
  "peerDependencies": {
    "vue": "^3.5.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.24.1",
    "@element-plus/icons-vue": "2.1.0",
    "@esbuild-kit/cjs-loader": "2.2.1",
    "@rushstack/eslint-patch": "^1.3.2",
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/vue3": "^7.0.0",
    "@storybook/vue3-vite": "^7.0.0",
    "@types/lodash-es": "4.17.11",
    "@types/node": "^20.0.0",
    "@types/sortablejs": "^1.15.8",
    "@unocss/eslint-config": "0.57.4",
    "@unocss/inspector": "0.54.0",
    "@unocss/transformer-variant-group": "0.57.4",
    "@unocss/vite": "0.54.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vitest/ui": "^1.0.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.3.0",
    "esbuild-register": "3.6.0",
    "eslint": "^8.39.0",
    "eslint-define-config": "1.24.1",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^27.0.0",
    "prettier": "^3.1.0",
    "typescript": "~5.0.0",
    "unplugin-element-plus": "0.8.0",
    "vite": "^4.5.0",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-progress": "0.0.7",
    "vitest": "^1.0.0",
    "vue": "^3.5.4",
    "vue-eslint-parser": "9.3.2",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "@cjx-low-code/components": "workspace:*",
    "@cjx-low-code/hooks": "workspace:*",
    "@cjx-low-code/locale": "workspace:*",
    "@cjx-low-code/theme-chalk": "workspace:*",
    "@pnpm/types": "8.4.0",
    "@types/gulp": "4.0.9",
    "@vue/runtime-core": "^3.5.4",
    "@vue/runtime-dom": "^3.5.4",
    "@vueuse/core": "9.1.0",
    "chalk": "5.0.1",
    "consola": "2.15.3",
    "element-plus": "2.8.4",
    "lodash-es": "4.17.21",
    "lodash-unified": "1.0.3",
    "reflect-metadata": "0.2.2",
    "rimraf": "3.0.2",
    "sass": "^1.93.2",
    "sortablejs": "1.15.0",
    "ts-morph": "14.0.0",
    "tsx": "4.7.1",
    "unplugin-vue-macros": "2.7.11",
    "vue-draggable-plus": "0.5.3"
  },
  "engines": {
    "node": ">= 16.0.0",
    "pnpm": ">=8.6.0"
  }
}